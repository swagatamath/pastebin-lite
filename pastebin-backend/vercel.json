{
  "version": 2,
  "builds": [{ "src": "api/index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/healthz", "dest": "api/index.js" },
    { "src": "/api/pastes", "dest": "api/index.js" },
    { "src": "/api/pastes/(.*)", "dest": "api/index.js" },
    { "src": "/p/(.*)", "dest": "api/index.js" }
  ]
}
